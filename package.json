{
  "name": "venturai",
  "version": "0.1.0",
  "private": true,
  "pnpm": {
    "overrides": {
      "convex": "^1.32.0"
    }
  },
  "scripts": {
    "dev": "turbo run dev --parallel",
    "dev:expo": "pnpm --filter @venturai/expo dev",
    "dev:web": "pnpm --filter @venturai/web dev",
    "dev:backend": "pnpm --filter @venturai/backend dev",

    "expo:prebuild": "pnpm --filter @venturai/expo prebuild",
    "dev:android": "pnpm --filter @venturai/expo android",
    "dev:ios": "pnpm --filter @venturai/expo ios",

    "lint": "turbo run lint",
    "format": "turbo run format",
    "typecheck": "turbo run typecheck",
    "build": "turbo run build",
    "test": "turbo run test",
    "clean": "git clean -xdf node_modules .turbo dist .expo .next .cache",
    "rename": "node scripts/rename-template.js"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.0",
    "ultracite": "6.0.1",
    "turbo": "^2.5.5",
    "typescript": "~5.9.3"
  },
  "packageManager": "pnpm@10.22.0"
}
