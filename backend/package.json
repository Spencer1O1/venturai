{
  "name": "@venturai/backend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./convex/_generated/api.d.ts",
      "default": "./convex/_generated/api.js"
    },
    "./api": {
      "types": "./convex/_generated/api.d.ts",
      "default": "./convex/_generated/api.js"
    },
    "./dataModel": {
      "types": "./convex/_generated/dataModel.d.ts",
      "default": "./convex/_generated/dataModel.js"
    }
  },
  "scripts": {
    "dev": "convex dev",
    "deploy": "convex deploy",
    "codegen": "convex codegen",
    "generateAuthKeys": "node scripts/generateKeys.mjs",
    "format": "biome check . --linter-enabled=false --fix",
    "lint": "biome lint . --write",
    "typecheck": "tsc --noEmit",
    "postinstall": "node ../scripts/symlink-env.js"
  },
  "dependencies": {
    "@auth/core": "0.37.0",
    "@convex-dev/auth": "^0.0.91",
    "convex": "^1.32.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@venturai/typescript": "workspace:*",
    "jose": "^6.1.3",
    "typescript": "~5.9.3"
  }
}
