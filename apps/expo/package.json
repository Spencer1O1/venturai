{
  "name": "@venturai/expo",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "build:android": "eas build --profile preview --platform android --local",
    "build:ios": "eas build --profile preview --platform ios --local",
    "clean": "git clean -xdf .expo .cache .turbo node_modules",
    "dev": "expo start",
    "eas": "eas",
    "expo": "expo",
    "format": "biome check . --linter-enabled=false --fix",
    "lint": "biome lint . --write",
    "postinstall": "node ../../scripts/symlink-env.js",
    "prebuild": "expo prebuild",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@convex-dev/auth": "^0.0.91",
    "@venturai/backend": "workspace:*",
    "convex": "^1.32.0",
    "expo": "~54.0.33",
    "expo-file-system": "~19.0.10",
    "expo-image-picker": "~17.0.7",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.23",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-nfc-manager": "^3.17.2",
    "react-native-safe-area-context": "~5.6.1",
    "react-native-screens": "~4.16.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.10",
    "@types/react-dom": "~19.1.10",
    "@venturai/typescript": "workspace:*",
    "sharp": "^0.34.5",
    "typescript": "~5.9.3"
  },
  "main": "expo-router/entry"
}
